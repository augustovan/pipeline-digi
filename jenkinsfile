            steps {
                     sh 'php --version'
                }
            }
        }

        stage ('Testing Stage') {

             steps {
                     sh 'php --version'
                }
            }
        }


        stage ('Deployment Stage') {
             steps {
                     sh 'php --version'
                }
            }
        }
    }
}